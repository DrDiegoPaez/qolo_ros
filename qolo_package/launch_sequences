
Execute in 110
--> take time from laptop Mac address
sudo ntpdate 192.168.13.100 
Restart chrony synchronisation:
service chrony restart

cd catkin_ws/
. devel/setup.bash

1 terminal
	rosocre
2 terminal:
	roslaunch qolo qolo.launch
3 terminal:
	./src/qolo_ros/run_scripts/run_rear_camera.sh
	# This includes the rear LIDAR
	OR
 	roslaunch rds_ros rds_rear_lrf.launch
 	OR
 	./src/qolo_ros/run_scripts/run_all_sensors.sh

Execute in 120:

1 terminal
		# This includes the frontal LIDAR
	./src/rds/rds_ros/launch/run_front_camera.sh
		
		OR only LIDAR:
	roslaunch rds_ros rds_front_lrf.launch
	

------------- RGBD Detection and Visualization -----------------
	
roslaunch movidius_ncs_launch ncs_camera.launch cnn_type:=tinyyolo_v1 camera:=realsense
rosrun kalman_bounding_boxes kalman_bounding_boxes_node

------------- ROSBAG recording  -----------------
###### ROSbag with RGBD and 3D points ######
	rosbag record -a

	rosbag record /tf /tf_static /front_lidar/velodyne_packets /front_lidar/velodyne_points /front_lidar/scan /rear_lidar/velodyne_packets /rear_lidar/velodyne_points /rear_lidar/scan /camera_front/color/image_raw /camera_front/color/camera_info /camera_front/depth/camera_info /camera_front/depth/image_rect_raw /camera_front/depth/color/points /camera_front/aligned_depth_to_color/camera_info /camera_front/aligned_depth_to_color/image_raw /camera_rear/color/image_raw /camera_rear/color/camera_info /camera_rear/depth/camera_info /camera_rear/depth/image_rect_raw /camera_rear/depth/color/points /camera_rear/aligned_depth_to_color/camera_info /camera_rear/aligned_depth_to_color/image_raw /rds_to_gui /qolo

###### ROSbag with Frontal RGBD and 3D points ######
	rosbag record -O 1m_1person.bag /tf /tf_static /front_lidar/velodyne_packets /front_lidar/velodyne_points /front_lidar/scan /camera_front/color/image_raw /camera_front/color/camera_info /camera_front/depth/camera_info /camera_front/depth/image_rect_raw /camera_front/depth/color/points /camera_front/aligned_depth_to_color/camera_info /camera_front/aligned_depth_to_color/image_raw /qolo

###### ROSbag with Frontal RGBD and 3D points ######
	rosbag record /velodyne_packets /camera/color/image_raw /camera_front/depth/image_rect_raw /front_lidar/laserscan

###### ROSbag with FT sensors ######
	rosbag record /rokubi_node_front/ft_sensor_measurements /rokubi_node_back/ft_sensor_measurements

------------- RDS  -----------------
	


	--- If building is necesary:
	cd ~/autonomy_ws/
	. make_rds.bash 

	--> take time from Main Qolo CPU
	sudo ntpdate 192.168.13.110
	Restart chrony synchronisation:
	service chrony restart

	rosrun rds_gui_ros rds_gui_ros_node


	To build it, go to the rds/rds folder and type
			make gui
			make rds_wrap
	go to --> catkin/src/rds 
		*Optionally for other pc: delete the package rds_ros
	And then use catkin_make in the top folder

	### Perhaps needed:
	Modify /usr/lib/x86_64-linux-gnu/cmake/SDL2/sdl2-config.cmake (White space at the end)
		BEFORE:    set(SDL2_LIBRARIES "-L${SDL2_LIBDIR}  -lSDL2 ")
		AFTER :    set(SDL2_LIBRARIES "-L${SDL2_LIBDIR}  -lSDL2")


------------- Velodyne points and LRF transformation ------------------
	roslaunch velodyne_pointcloud VLP16_points.launch 
	rosrun rds_ros rds_ros_lidar_to_lrf_node 
	rosbag record /laserscan /velodyne_points
	rosbag record /qolo /velodyne_packets /rds_to_gui

Velodyne New package isntallation:
	sudo rm -r src/velodyne build/velodyne devel/include/velodyne_* devel/lib/velodyne_* devel/lib/libvelodyne_* devel/lib/libdriver_nodelet.so devel/share/velodyne_*
		Known Working version with RDS: git checkout 7f52b3586a2833b4a48bd2539a862adedb161baf
	sudo apt-get install ros-kinetic-velodyne 
	catkin_make clean
	catkin_make

	sudo apt-get remove ros-kinetic-velodyne ros-kinetic-velodyne-driver ros-kinetic-velodyne-laserscan ros-kinetic-velodyne-msgs ros-kinetic-velodyne-pointcloud 
	cd ~/autonomy_ws/src/velodyne
	git clone https://github.com/ros-drivers/velodyne.git
	cd ~/catkin_ws/
	rosdep install --from-paths src --ignore-src --rosdistro kinetic -y
	catkin_make clean
	catkin_make
	
